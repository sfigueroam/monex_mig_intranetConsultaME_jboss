	<groupId>cl.gob.tgr.monex</groupId>
	<artifactId>intranetConsultasME</artifactId>
	<packaging>ear</packaging>

	<build>
		<finalName>intranetConsultasME</finalName>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-ear-plugin</artifactId>
					<version>3.0.1</version>
					<configuration>
						<earSourceDirectory>EarContent</earSourceDirectory>
						<version>6</version>
						<defaultLibBundleDir>APP-INF/lib</defaultLibBundleDir>
						<modules>
							<webModule>
								<groupId>intranetconsultasme.webapplication</groupId>
								<artifactId>ConsultasME</artifactId>
								<bundleFileName>ConsultasME.war</bundleFileName>
							</webModule>
							<ejbModule>
								<groupId>cl.tesoreria.monex.pkgconsultasmonex</groupId>
								<artifactId>PkgConsultasMonExEJB</artifactId>
								<bundleFileName>PkgConsultasMonExEJB.jar</bundleFileName>
							</ejbModule>
							<ejbModule>
								<groupId>cl.tesoreria.monex</groupId>
								<artifactId>PkgCMonExEJB</artifactId>
								<bundleFileName>PkgCMonExEJB.jar</bundleFileName>
							</ejbModule>					
						</modules>
							<displayName>intranetConsultasME</displayName>
							<generateApplicationXml>true</generateApplicationXml>
						</configuration>
					</plugin>
				<!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>
											org.apache.maven.plugins
										</groupId>
										<artifactId>
											maven-ear-plugin
										</artifactId>
										<versionRange>
											[3.0.1,)
										</versionRange>
										<goals>
											<goal>
												generate-application-xml
											</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
